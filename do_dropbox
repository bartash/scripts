#!/usr/bin/env bash
# copy dropbox files
MAC="asherman-MBP"
if [[ ${HOSTNAME} != ${MAC}* ]];then
	echo you are on the wrong machine, you should be on ${MAC}
	exit 1
fi


rsync -rv --delete --update -e ssh asherman-desktop.sf.cloudera.com:Dropbox ~

rsync -rv --delete --update -e ssh ~/BackDropbox asherman-desktop.sf.cloudera.com:

